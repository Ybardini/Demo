        "for i in range(len(ks)):\n",
        "\n",
        "  fit = data[0]*np.exp(ks[i]*times)\n",
        "\n",
        "  chi_squares[i] = chi_squared(data,fit)\n",
        " \n",
        "plt.plot(ks, chi_squares);\n",
        "min = chi_squares.min()"
