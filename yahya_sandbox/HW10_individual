    {
      "cell_type": "code",
      "source": [
        "#Modified chi_squared function\n",
        "def chi_squared2(params, data, times):\n",
        "  \n",
        "  k = params[0]\n",
        "  chi_sq = 0\n",
        "  fit = data[0]*np.exp(k*times)\n",
        "\n",
        "  for i in range(len(fit)):\n",
        "    chi_sq+=((data[i]-fit[i])**2)/fit[i]\n",
        "  \n",
        "  return chi_sq\n"
      ],
      "metadata": {
        "id": "5_X5-0f3QtFP"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#scipy.optimize.minimize(func,x0,args=(), bounds=[])\n",
        "scipy.optimize.minimize(chi_squared2, [0.027], args=(data, times), bounds=[(0.02, 0.03)])\n"
      ],
      "metadata": {
        "colab": {
