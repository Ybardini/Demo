        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "best_k = ans.x[0]\n",
        "best_A0 = ans.x[1]\n",
        "\n",
        "fit = best_A0*np.exp(best_k*times)\n",
        "plt.plot(times, fit)\n",
        "\n",
        "plt.plot(times,data,'.')\n",
        "\n",
