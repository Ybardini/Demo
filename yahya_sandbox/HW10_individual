        "#Areas chi_squared function\n",
        "def chi_squared_A(params, data, times):\n",
        "  \n",
        "  k = params[0]\n",
        "  A0 = params[1]\n",
        "  chi_sq = 0\n",
        "  fit = A0*np.exp(k*times)\n",
        "\n",
        "  for i in range(len(fit)):\n",
        "    chi_sq+=((areas[i]-fit[i])**2)/fit[i]\n",
        "  \n",
        "  return chi_sq\n"
