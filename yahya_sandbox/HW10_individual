    {
      "cell_type": "code",
      "source": [
        "#Modify chi_squared again, to accomodate the new A0 paramter"
      ],
      "metadata": {
        "id": "xIKMZiWpTk2t"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Modified chi_squared function\n",
        "def chi_squared3(params, data, times):\n",
        "  \n",
        "  k = params[0]\n",
        "  #print(k)\n",
        "  A0 = params[1]\n",
        "  chi_sq = 0\n",
        "  fit = A0*np.exp(k*times)\n",
        "\n",
        "  for i in range(len(fit)):\n",
        "    chi_sq+=((data[i]-fit[i])**2)/fit[i]\n",
        "  \n",
        "  return chi_sq\n"
      ],
      "metadata": {
        "id": "P0YI41bwTpwl"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "ans = scipy.optimize.minimize(chi_squared3, [0.027, 7700], args=(data, times),\n",
        "                              bounds=[(.01, .05), (5000, 10000)])\n",
        "ans.x"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "RzTkdg01SI7E",
        "outputId": "b2fac533-9511-463e-c8c6-23e6edf52558"
